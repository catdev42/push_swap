
{
        "version": "0.2",
        "configurations": [
            {
                "name": "Debug push_swap",
                "type": "cppdbg",
                "request": "launch",
                "program": "${workspaceFolder}/push_swap", // Path to your push_swap executable
                "args": ["1", "3", "2", "4", "8", "7", "9"], // Command-line arguments if any
                "stopAtEntry": false,
                "cwd": "${workspaceFolder}",
                "environment": [],
                "externalConsole": false,
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ],
                // "preLaunchTask": "Build push_swap" // Task for building your program
            }
        ]
    }
    